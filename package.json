{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:lib": "vite build -c vite-lib.config.ts && npm run after:build",
    "after:build": "node ./scripts/import-css.js && node ./scripts/generate-dts.js && node ./scripts/release.js",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "docs:dev": "vitepress dev docs --port 5174 --host 0.0.0.0",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@devui-design/icons": "^1.4.0",
    "@floating-ui/dom": "^1.6.12",
    "@vue/shared": "^3.5.13",
    "clipboard-copy": "^4.0.1",
    "devui-theme": "^0.0.7",
    "highlight.js": "^11.11.0",
    "lodash": "^4.17.21",
    "markdown-it": "12.2.0",
    "openai": "^4.77.0",
    "sass": "^1.83.0",
    "three": "^0.171.0",
    "vue": "^3.5.13",
    "vue-devui": "^1.6.29",
    "vue-i18n": "^11.0.0-beta.2"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.7.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "fs-extra": "^11.2.0",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.3.3",
    "sass": "^1.83.0",
    "sass-loader": "^16.0.4",
    "typescript": "~5.6.3",
    "vite": "^6.0.1",
    "vite-plugin-vue-devtools": "^7.6.5",
    "vitepress": "^1.5.0",
    "vitepress-theme-demoblock": "^3.0.7",
    "vue-tsc": "^2.1.10"
  }
}
